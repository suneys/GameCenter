<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><!DOCTYPE html><html><head>    <meta charset="utf-8"/>    <meta name="viewport" content="width=device-width"/>    <meta name="renderer" content="webkit"/>    <title>找回密码-海报助手</title>    <meta name="description" content="海报助手、数字广告牌、数字灯箱、数字海报、数字菜单、数字展架、数字白板、海报发布系统 研发 制造 生产"/>    <meta name="keywords" content="海报助手、数字广告牌、数字灯箱、数字海报、数字菜单、数字展架、数字白板、海报发布系统 研发 制造 生产"/>    <link href="${pageContext.request.contextPath }/css/getpwd.css" rel="stylesheet"/>    <script src="${pageContext.request.contextPath }/js/jquery-1.9.0.min.js"></script>    <script src="${pageContext.request.contextPath }/js/mesageinfo.js"></script>    <script src="${pageContext.request.contextPath }/js/public.js"></script>    <script src="${pageContext.request.contextPath }/js/adv_GetPassword_email.js"></script>    <script src="${pageContext.request.contextPath }/js/adv_GetPassword_phone.js"></script>    <script src="${pageContext.request.contextPath }/js/jquery.autoComplete.js"></script></head><body><header class="sso_header"><a href="#" class="logo"></a></header><section class="content_box" id="findPwd2" style="display: block;">    <div class="findPwd">        <form action="" method="" id="from2">            <ul class="form_head clearfix">                <li class="active" id="elearfix1" style="margin-right:42px;"><span class="icon_step step1"></span>输入邮箱地址                </li>                <li id="elearfix2" style="margin-right:53px;width:59px;"><span class="icon_step step2"></span>验证邮箱</li>                <li style="width:59px"><span class="icon_step step3"></span>重置密码</li>            </ul>            <div class="form_body" id="emailFindStep1">                <div class="input_item clearfix" data-propertyname="email" data-controltype="Email"                     style="display: block;">                    <input type="text" class="input input_white" id="txtEmail" name="" placeholder="请输入注册时使用的邮箱地址"                           data-required="required" autocomplete="off">                    <span id="eTip1" class="input_tips" style="display:none;">请输入正确的邮箱地址</span>                </div>                <div class="input_item clearfix" data-propertyname="email" data-controltype="Email"                     style="display: block;">                    <input type="text" class="input input_white" id="txtName" name="" placeholder="请输入用户名"                           data-required="required" >                    <span id="eTip3" class="input_tips" style="display:none;">用户名不能为空</span>                </div>                <div class="input_item clearfix" data-propertyname="request_form_verifyCode"                     data-controltype="VerifyCode">                    <input type="text" id="txtEmailCode" maxlength="4" class="input input_white fl"                           style="width:130px; display:block;" placeholder="请输入验证码" data-required="required"                           autocomplete="off">                    <img class="yzm" id="yzm2" src="${pageContext.request.contextPath }/getAuthCode"                         onclick="this.src = this.src + '?' + new Date().getTime();">                    <a rel="nofollow" href="javascript:;" onclick="$('#yzm2').click();" class="reflash"></a>                    <span id="eTip2" class="input_tips" style="display:none;">请输入4位图形验证码</span>                </div>                <div class="input_item clearfix" data-propertyname="submit" data-controltype="Botton"                     style="display: block;">                    <input type="button" id="BtnByEmail" onclick="SendEmailCode()"                           class="btn btn_green btn_active btn_block" value="找回密码">                </div>            </div>            <div class="form_body" id="emailFindStep2" style="display: none;">                <div class="input_item clearfix">                    <h3 class="reset_mail">密码重置邮件已发送至你的邮箱：<span class="des_mail"></span></h3>                    <p class="reset_tips">请在30分钟内登录你的邮箱接收邮件,链接激活后可重置密码。</p>                </div>                <div class="input_item clearfix">                    <input type="button" onclick="gotoVerify()" class="btn btn_green btn_active btn_block"                           value="登录邮箱查看"/>                </div>            </div>        </form>    </div></section><footer><h4 class="slogan">— 推荐在360、谷歌、火狐浏览器使用 —</h4></footer><input type="hidden" id="sendtype" name="sendtype" value="2"/><input type="hidden" id="gotoUrl" name="gotoUrl" value="http://mail.163.com"/><link rel="stylesheet" type="text/css" href="http://image.smt-view.com/Computer/css/shortcut.css?ver=V1.189536"><script type="text/javascript" src="http://image.smt-view.com/Computer/js/scrop.js?ver=V1.189536"></script><ul class="shortcut">    <li class="contact">        <span class="shortcut-text" title="联系我们">联系我们</span>        <div class="shortcut-inner">            <div class="shortcut-box">                <div class="shortcut-arrow">                </div>                <div class="shortcut-item cpzx">                    <a href="http://wpa.qq.com/msgrd?v=3&amp;uin=28444765&amp;site=qq&amp;menu=yes" target="_blank">                        <h4 class="shortcut-item-title">客服QQ</h4>                        <div class="shortcut-item-pic"></div>                    </a>                </div>                <div class="shortcut-item jszc">                    <a href="http://wpa.qq.com/msgrd?v=3&amp;uin=28444765&amp;site=qq&amp;menu=yes" target="_blank">                        <h4 class="shortcut-item-title">技术QQ</h4>                        <div class="shortcut-item-pic"></div>                    </a>                </div>                <div class="clearfix"></div>                <h2 class="shortcut-tel">客服专线：13246883050</h2>                <h2 class="shortcut-tel">技术专线：13246883050</h2>            </div>        </div>    </li>    <li class="gotop" style="display: block;">        <span class="shortcut-text" title="回到顶部">回到顶部</span>    </li></ul></body></html>