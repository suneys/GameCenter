<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><!DOCTYPE html><html><head>    <meta charset="utf-8"/>    <meta name="viewport" content="width=device-width"/>    <meta name="renderer" content="webkit"/>    <title>找回密码-海报助手</title>    <meta name="description" content="海报助手、数字广告牌、数字灯箱、数字海报、数字菜单、数字展架、数字白板、海报发布系统 研发 制造 生产"/>    <meta name="keywords" content="海报助手、数字广告牌、数字灯箱、数字海报、数字菜单、数字展架、数字白板、海报发布系统 研发 制造 生产"/>    <link href="${pageContext.request.contextPath }/css/getpwd.css" rel="stylesheet"/>    <script src="${pageContext.request.contextPath }/js/jquery-1.9.0.min.js"></script>    <script src="${pageContext.request.contextPath }/js/mesageinfo.js"></script>    <script src="${pageContext.request.contextPath }/js/public.js"></script>    <script src="${pageContext.request.contextPath }/js/adv_ResetPassword_email.js"></script>    <script src="${pageContext.request.contextPath }/js/jquery.autoComplete.js"></script></head><body><header class="sso_header"><a href="#" class="logo"></a></header><section class="content_box" id="findPwd2" style="display: block;">    <div class="findPwd">        <form action="${pageContext.request.contextPath }/update" method="post" id="from2">            <ul class="form_head clearfix">                <li  class="active" id="elearfix1" style="margin-right:42px;"><span class="icon_step step1"></span>输入邮箱地址                </li>                <li id="elearfix2" class="active" style="margin-right:53px;width:59px;"><span class="icon_step  step2"></span>验证邮箱</li>                <li  class="active" style="width:59px"><span   class="icon_step  active step3"></span>重置密码</li>            </ul>            <div class="form_body" id="emailFindStep1">                <div class="input_item clearfix" data-propertyname="email" data-controltype="Email"                     style="display: block;">                    <input type="password" class="input input_white" id="txtPassword1" name="" placeholder="设置新密码"                           data-required="required" autocomplete="off">                    <span id="eTip1" class="input_tips" style="display:none;">密码不能为空</span>                </div>                <div class="input_item clearfix" data-propertyname="email" data-controltype="Email"                     style="display: block;">                    <input type="password" class="input input_white" id="txtPassword2" name="password" placeholder="确认新密码"                           data-required="required" >                    <span id="eTip2" class="input_tips" style="display:none;">密码不能为空</span>                </div>                <div class="input_item clearfix" data-propertyname="submit" data-controltype="Botton"                     style="display: block;">                    <input type="button" id="BtnResetPassword" onclick="SendNewPassword()"                           class="btn btn_green btn_active btn_block" value="修改密码">                </div>            </div>            <input type="hidden"  id="userID" name="userID" value="${userId}"/>        </form>    </div></section><footer><h4 class="slogan">— 推荐在360、谷歌、火狐浏览器使用 —</h4></footer><input type="hidden" id="sendtype" name="sendtype" value="2"/><input type="hidden" id="gotoUrl" name="gotoUrl" value="http://mail.163.com"/><link rel="stylesheet" type="text/css" href="http://image.smt-view.com/Computer/css/shortcut.css?ver=V1.189536"><script type="text/javascript" src="http://image.smt-view.com/Computer/js/scrop.js?ver=V1.189536"></script><ul class="shortcut">    <li class="contact">        <span class="shortcut-text" title="联系我们">联系我们</span>        <div class="shortcut-inner">            <div class="shortcut-box">                <div class="shortcut-arrow">                </div>                <div class="shortcut-item cpzx">                    <a href="http://wpa.qq.com/msgrd?v=3&amp;uin=28444765&amp;site=qq&amp;menu=yes" target="_blank">                        <h4 class="shortcut-item-title">客服QQ</h4>                        <div class="shortcut-item-pic"></div>                    </a>                </div>                <div class="shortcut-item jszc">                    <a href="http://wpa.qq.com/msgrd?v=3&amp;uin=28444765&amp;site=qq&amp;menu=yes" target="_blank">                        <h4 class="shortcut-item-title">技术QQ</h4>                        <div class="shortcut-item-pic"></div>                    </a>                </div>                <div class="clearfix"></div>                <h2 class="shortcut-tel">客服专线：13246883050</h2>                <h2 class="shortcut-tel">技术专线：13246883050</h2>            </div>        </div>    </li>    <li class="gotop" style="display: block;">        <span class="shortcut-text" title="回到顶部">回到顶部</span>    </li></ul></body></html>